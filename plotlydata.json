{
    "layout":{
        "template": "plotly_dark",
        "legend":{
            "visible":false
        },
        "title":{
            "text":"_TICKERNAME_",
            "y":0.95,
            "x":0.5,
            "xanchor":"center",
            "yanchor":"top",
            "font": {
                "size":40,
                "family":"Arial"
            }
        },
        "xaxis":{
            "rangeslider":{
                "yaxis":{
                    "rangemode":"auto"
                },
                "visible":false,
                "thickness":0.07
            }
        },
        "yaxis":{
            "fixedrange":false
        },
        "margin":{
            "l":20,
            "r":20,
            "t":60,
            "b":20
        }
    },
    
    "stockdata":{
        
        "candle_data":{
            "x":"_DATES_",
            "open":"_OPENDATA_",
            "high":"_HIGHDATA_",
            "low":"_LOWDATA_",
            "close":"_CLOSEDATA_",
            "increasing":{
                "fillcolor":"yellow", 
                "line":{
                    "color":"yellow",
                    "width":1
                }
            },
            "decreasing":{
                "fillcolor":"grey", 
                "line":{
                    "color":"grey",
                    "width":1
                }
            }
        }
    },

    "indicators":{

        "line":{
            "x": "_DATES_",
            "y": "_YVALUES_",
            "line":{
                "color":"None",
                "width":1
            },
            "name": "_DATANAME_"
        },

        "upperband":{
            "x": "_DATES_",
            "y": "_YVALUES_",
            "line":{
                "dash": "dash",
                "color":"yellow",
                "width":1
            },
            "name": "_DATANAME_"
        },

        "lowerband":{
            "x": "_DATES_",
            "y": "_YVALUES_",
            "line":{
                "dash": "dash",
                "color":"grey",
                "width":1
            },
            "fillcolor": "rgba(0, 0, 0, 0.16)",
            "fill": "tonexty",
            "name": "_DATANAME_",
            "opacity": 0.1
        },

        "bar":{
            "x":"_DATES_",
            "y":"_YVALUES_",
            "name":"_DATANAME_"
        },

        "hrect":{
            "y0":"_MINVAL_",
            "y1":"_MAXVAL_",
            "fillcolor":"lightgray",
            "opacity":0.1
        }

    }

}